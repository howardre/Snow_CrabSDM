@article{lester.etal_2010,
  title = {Science in Support of Ecosystem-Based Management for the {{U}}.{{S}}. {{West Coast}} and Beyond},
  author = {Lester, Sarah E. and McLeod, Karen L. and Tallis, Heather and Ruckelshaus, Mary and Halpern, Benjamin S. and Levin, Phillip S. and Chavez, Francisco P. and Pomeroy, Caroline and McCay, Bonnie J. and Costello, Christopher and Gaines, Steven D. and Mace, Amber J. and Barth, John A. and Fluharty, David L. and Parrish, Julia K.},
  date = {2010-03},
