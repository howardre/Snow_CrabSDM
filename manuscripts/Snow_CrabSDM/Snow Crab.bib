  author = {Bacheler, Nathan M. and Ciannelli, Lorenzo and Bailey, Kevin M. and Duffy-Anderson, Janet T.},
  date = {2010},
  journaltitle = {Fisheries Oceanography},
  volume = {19},
  number = {2},
