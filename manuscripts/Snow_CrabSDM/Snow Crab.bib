@article{bryan.etal_2014,
  title = {Quantitative Video Analysis of Flatfish Herding Behavior and Impact on Effective Area Swept of a Survey Trawl},
  author = {Bryan, David R. and Bosley, Keith L. and Hicks, Allan C. and Haltuch, Melissa A. and Wakefield, W. Waldo},
