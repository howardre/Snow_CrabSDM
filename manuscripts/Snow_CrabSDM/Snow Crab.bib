@article{brady.etal_2017,
  title = {Emergent Anthropogenic Trends in {{California Current}} Upwelling},
  author = {Brady, Riley X. and Alexander, Michael A. and Lovenduski, Nicole S. and Rykaczewski, Ryan R.},
  date = {2017},
  journaltitle = {Geophysical Research Letters},
  shortjournal = {Geophys Res Lett},
  volume = {44},
  number = {10},
  pages = {5044--5052},
