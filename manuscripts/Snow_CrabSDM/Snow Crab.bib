@article{stawitz.etal_2015,
  title = {A State-Space Approach for Detecting Growth Variation and Application to {{North Pacific}} Groundfish},
  author = {Stawitz, Christine C. and Essington, Timothy E. and Branch, Trevor A. and Haltuch, Melissa A. and Hollowed, Anne B. and Spencer, Paul D.},
  date = {2015-09},
  journaltitle = {Canadian Journal of Fisheries and Aquatic Sciences},
  shortjournal = {Can J Fish Aquat Sci},
  volume = {72},
