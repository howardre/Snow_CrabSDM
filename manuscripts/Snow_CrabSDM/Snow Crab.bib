@article{nielsen.etal_2021,
  title = {Responses of Ichthyoplankton Assemblages to the Recent Marine Heatwave and Previous Climate Fluctuations in Several {{Northeast Pacific}} Marine Ecosystems},
  author = {Nielsen, Jens M. and Rogers, Lauren A. and Brodeur, Richard D. and Thompson, Andrew R. and Auth, Toby D. and Deary, Alison L. and Duffy‐Anderson, Janet T. and Galbraith, Moira and Koslow, J. Anthony and Perry, R. Ian},
  date = {2021-02},
  journaltitle = {Global Change Biology},
  shortjournal = {Glob. Change Biol.},
