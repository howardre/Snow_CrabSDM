@article{stabeno.etal_2008,
  title = {An Examination of the Physical Variability around the {{Pribilof Islands}} in 2004},
  author = {Stabeno, P. J. and Kachel, N. and Mordy, C. and Righi, D. and Salo, S.},
  date = {2008-08-01},
  journaltitle = {Deep Sea Research Part II: Topical Studies in Oceanography},
  shortjournal = {Deep Sea Research Part II: Topical Studies in Oceanography},
  series = {The {{Marine Ecosystem}} of the {{Pribilof Domain}}, {{Southeastern Bering Sea}}},
  volume = {55},
