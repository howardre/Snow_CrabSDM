@article{schirripa.colbert_2006,
  title = {Interannual Changes in Sablefish ({{{\emph{Anoplopoma}}}}{\emph{ Fimbria}}) Recruitment in Relation to Oceanographic Conditions within the {{California Current System}}},
  author = {Schirripa, Michael J. and Colbert, J. J.},
