@article{berger.etal_2020,
  title = {Incoherent Dimensionality in Fisheries Management: Consequences of Misaligned Stock Assessment and Population Boundaries},
  shorttitle = {Incoherent Dimensionality in Fisheries Management},
  author = {Berger, Aaron M and Deroba, Jonathan J and Bosley, Katelyn M and Goethel, Daniel R and Langseth, Brian J and Schueller, Amy M and Hanselman, Dana H},
  date = {2020-12-22},
  journaltitle = {ICES Journal of Marine Science},
