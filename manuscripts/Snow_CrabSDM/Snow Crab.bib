@article{link.etal_2011,
  title = {Guidelines for Incorporating Fish Distribution Shifts into a Fisheries Management Context},
  author = {Link, Jason S. and Nye, Janet A. and Hare, Jonathan A.},
  date = {2011},
  journaltitle = {Fish and Fisheries},
