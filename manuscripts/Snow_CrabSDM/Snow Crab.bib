}

@article{litzow_2006,
  title = {Climate Regime Shifts and Community Reorganization in the {{Gulf}} of {{Alaska}}: How Do Recent Shifts Compare with 1976/1977?},
  shorttitle = {Climate Regime Shifts and Community Reorganization in the {{Gulf}} of {{Alaska}}},
  author = {Litzow, Michael A.},
  date = {2006-01-01},
  journaltitle = {ICES Journal of Marine Science},
