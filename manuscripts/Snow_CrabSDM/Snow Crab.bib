@article{jacox.etal_2014,
  title = {Spatially Resolved Upwelling in the {{California Current System}} and Its Connections to Climate Variability},
  author = {Jacox, M. G. and Moore, A. M. and Edwards, C. A. and Fiechter, J.},
  date = {2014-05-16},
  journaltitle = {Geophysical Research Letters},
