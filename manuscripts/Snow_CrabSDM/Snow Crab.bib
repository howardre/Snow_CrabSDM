  author = {Hermann, Albert J. and Curchitser, Enrique N. and Haidvogel, Dale B. and Dobbins, Elizabeth L.},
  date = {2009-11-15},
  journaltitle = {Deep Sea Research Part II: Topical Studies in Oceanography},
  shortjournal = {Deep Sea Research Part II: Topical Studies in Oceanography},
  series = {Physical and {{Biological Patterns}}, {{Processes}}, and {{Variability}} in the {{Northeast Pacific}}.},
  volume = {56},
  number = {24},
  pages = {2427--2443},
  issn = {0967-0645},
  doi = {10.1016/j.dsr2.2009.02.005},
  url = {http://www.sciencedirect.com/science/article/pii/S0967064509000459},
  urldate = {2021-01-10},
  abstract = {A set of spatially nested circulation models is used to explore interannual change in the northeast Pacific (NEP) during 1997–2002, and remote vs. local influence of the 1997–1998 El Niño on this region. Our nested set is based on the primitive equations of motion, and includes a basin-scale model of the north Pacific at ∼40-km resolution (NPac), and a regional model of the Northeast Pacific at ∼10-km resolution. The NEP model spans an area from Baja California through the Bering Sea, from the coast to ∼2000-km offshore. In this context, “remote influence” refers to effects driven by changes in ocean velocity and temperature outside of the NEP domain; “local influence” refers to direct forcing by winds and runoff within the NEP domain. A base run of this model using hindcast winds and runoff for 1996–2002 replicates the dominant spatial modes of sea-surface height anomalies from satellite data, and coastal sea level from tide gauges. We have performed a series of sensitivity runs with the NEP model for 1997–1998, which analyze the response of coastal sea level to: (1) hindcast winds and coastal runoff, as compared to their monthly climatologies and (2) hindcast boundary conditions (from the NPac model), as compared to their monthly climatologies. Results indicate penetration of sea-surface height (SSH) from the basin-scale model into the NEP domain (e.g., remote influence), with propagation as coastal trapped waves from Baja up through Alaska. Most of the coastal sea-level anomaly off Alaska in El Niño years appears due to direct forcing by local winds and runoff (local influence), and such anomalies are much stronger than those produced off California. We quantify these effects as a function of distance along the coastline, and consider how they might impact the coastal ecosystems of the NEP.},
  langid = {english},
  keywords = {Coastal currents,Coastal oceanography,El Niño phenomena,Mathematical models,Northeast Pacific,Physical oceanography},
  file = {C\:\\Users\\howar\\Zotero\\storage\\LIHZAPME\\Hermann et al. - 2009 - A comparison of remote vs. local influence of El N.pdf;C\:\\Users\\howar\\Zotero\\storage\\MX7DJ6AP\\S0967064509000459.html}
}

